            "preLaunchTask": "C/C++: g++.exe アクティブなファイルのビルド",
            "miDebuggerArgs": "",
            "logging": { "trace": false, "traceResponse": false, "engineLogging": false },
            "windows": {
                "MIMode": "gdb",
                "miDebuggerPath": "C:\\Program Files (x86)\\mingw64\\bin\\gdb.exe",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
